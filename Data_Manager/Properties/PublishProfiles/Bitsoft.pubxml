<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>localhost/bitsoft</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>localhost/bitsoft</MSDeployServiceURL>
    <DeployIisAppPath>Default Web Site/bitsoft</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>InProc</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName />
    <_SavePWD>False</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="False">
          <Destination Path="Data Source=SA\SQLEXPRESS;Initial Catalog=Apps_Service;User ID=sa;Password=dbms" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="Data_Manager.Models.UsersContext, Data_Manager" Origin="Convention" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="Entities_Data" Order="2" Enabled="False">
          <Destination Path="Data Source=SA\SQLEXPRESS;Initial Catalog=POS_Nagori;User ID=sa;Password=dbms" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="Data_Manager.Models.Entities_Data, Data_Manager" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="conn" Order="4" Enabled="False">
          <Destination Path="Data Source=SA\SQLEXPRESS;Initial Catalog=POS_Nagori;User ID=sa;Password=dbms" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=SA\MSSQLSERVER2012;Initial Catalog=POS_Nagori;User ID=sa;Password=dbms;Application Name=EntityFramework" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\conn_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=SA\MSSQLSERVER2012;Initial Catalog=POS_Nagori;User ID=sa;Password=dbms;MultipleActiveResultSets=True;Application Name=EntityFramework" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)conn-Web.config Connection String">
      <ParameterValue>data source=SA\SQLEXPRESS;initial catalog=POS_Nagori;user id=sa;password=dbms;MultipleActiveResultSets=True</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=SA\SQLEXPRESS;Initial Catalog=Apps_Service;User ID=sa;Password=dbms</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)Entities_Data-Web.config Connection String">
      <ParameterValue>metadata=res://*/Models.DataModel.csdl|res://*/Models.DataModel.ssdl|res://*/Models.DataModel.msl;provider=System.Data.SqlClient;provider connection string="Data Source=SA\SQLEXPRESS;Initial Catalog=POS_Nagori;User ID=sa;Password=dbms"</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="DefaultConnection" />
  </ItemGroup>
</Project>