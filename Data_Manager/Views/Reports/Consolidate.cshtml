@{
    ViewBag.Title = "Consolidate";
}


<h2>Consolidate</h2>
<div class="well row">
    <div class="col-lg-2">
        
    </div>
    <div class="col-lg-2">
        <div class="editor-label">
            Users
        </div>
        <div class="editor-field">
            @Html.DropDownList("EmpID", null, "Select User")
            @*<input />*@
        </div>
    </div>
    <div class="col-lg-2">
        <div class="editor-label">
            From Date
        </div>
        <div class="editor-field">
            <div class="input-group">
                <div class="input-group-addon"><span class="icon-calendar-empty"></span></div>
                <input name="fromdate" id="fromdate" type="text" class="datepicker1 form-control" value="mm/dd/yyyy" />
                @*<input name="fromdate" id="fromdate" />*@
            </div>
        </div>
    </div>
    <div class="col-lg-2">
        <div class="editor-label">
            To Date
        </div>
        @* <div class="editor-field">*@
        <div class="input-group">
            <div class="input-group-addon"><span class="icon-calendar-empty"></span></div>
            <input name="todate" id="todate" type="text" class="datepicker1 form-control" value="mm/dd/yyyy" />
        </div>
        @*  </div>*@
    </div>
    <div class="col-lg-2">
        <div class="editor-label">
            <br />
        </div>
       <button class="btn btn-success"><i class="iconicon-search"></i> Search </button>
    </div>
</div>
