<div class="container">   
    <div class="row">
        @using (Html.BeginForm("Index", "Reports_Chart", FormMethod.Post))
        {   
            <div title="Selected Date" class="block-fill-white col-md-3 input-group">
                
                <div class="input-group-addon"><span class="icon-calendar-empty"> </span> </div>
                
                <input style="font-weight: bold;font-size: 15px;color: #f70c0ccf;" name="date1" value="@String.Format("{0:dd/MMM/yy}", @TempData["date"])" onchange="submit()" type="text" class="datepicker1 form-control " id="dp1519122175306">
            </div>
        }
            <ol class="breadcrumb">
                <li><a href="#">Modules</a></li>
                <li><a href="#">Reports Charts</a></li>
                
            </ol>
        
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
            <div class="block">
                <div class="header">
                    <h2><i class="widget-icon widget-icon-dark icon-list"> </i> Daily Sales </h2> <a class="btn btn-" title="Go to Details" href='@Url.Action("Index", "SaleMaster")'> <span class="icon-location-arrow"></span></a>
                </div>
                <div class="content">
                    <center>
                        <div title="" style="width: 20%; min-width: 185px;" class="hp-info hp-simple  hp-inline">
                            <span class="hp-main">
                                <span class=""></span>@{ decimal sale = @ViewBag.Sale; sale = ((sale / 250000) * 100) ;
                                }@sale.ToString("n2")% <i title="Sale Amount" >(@String.Format("{0:n0}", @ViewBag.Sale))</i>
                            </span>
                            <div class="hp-sm">
                                <div class="progress progress-small  shine-me">
                                    <div id="pro" class="Sale progress-bar progress-bar-danger" role="progressbar" aria-valuenow="@sale" aria-valuemin="0" aria-valuemax="100"></div>
                                </div>
                            </div>
                        </div>
                    </center>
                </div>
            </div>
        </div>
        

        <div class="col-md-3">
            <div class="block">
                <div class="header">
                    <h2><i class="widget-icon widget-icon-dark icon-list"> </i> Daily Purchase</h2><a class="btn btn-" title="Go to Details" href='@Url.Action("Index", "StockMaster")'> <span class="icon-location-arrow"></span></a>
                </div>
                <div class="content">
                    <center>
                        <div title="" style="width: 20%; min-width: 185px;" class="hp-info hp-simple  hp-inline">
                            <span class="hp-main">
                                <span class=""></span>@{ decimal Purchase = @ViewBag.Purchase; Purchase = ((Purchase / 500000) * 100);
                                }@Purchase.ToString("n2")% <i title="Purchase Amount">(@String.Format("{0:n0}", @ViewBag.Purchase))</i>
                            </span>

                            <div class="hp-sm">
                                <div class="progress progress-small  shine-me">
                                    <div id="pro" class="Purchase progress-bar progress-bar-success" role="progressbar" aria-valuenow="37.42" aria-valuemin="0" aria-valuemax="100"></div>
                                </div>
                            </div>
                        </div>
                    </center>
                </div>
            </div>
        </div>
        <div class="col-md-3">
            <div class="block">
                <div class="header">
                    <h2><i class="widget-icon widget-icon-dark icon-list"> </i> Daily Expense</h2><a class="btn btn-" title="Go to Details" href='@Url.Action("Index", "MasterExpence")'> <span class="icon-location-arrow"></span></a>
                </div>
                <div class="content">
